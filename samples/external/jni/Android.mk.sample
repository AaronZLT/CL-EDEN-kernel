LOCAL_PATH := $(call my-dir)

include $(CLEAR_VARS)
LOCAL_MODULE := enn_public_api_cpp_
LOCAL_SRC_FILES := ${LOCAL_PATH}/lib64/libenn_public_api_cpp.so
include $(PREBUILT_SHARED_LIBRARY)

include $(CLEAR_VARS)
LOCAL_MODULE := enn_sample_external

LOCAL_C_INCLUDES += \
                    ${LOCAL_PATH} \
                    ${LOCAL_PATH}/include

LOCAL_LDLIBS := -llog
LOCAL_ARM_NEON := true
LOCAL_CFLAGS += -Wall -mfloat-abi=softfp -std=c++14 -O3
LOCAL_CPPFLAGS += -fexceptions -frtti -DANDROID_STL=gnustl_shared

LOCAL_SRC_FILES := enn_sample_external.cc
LOCAL_STATIC_LIBRARIES := enn_public_api_cpp_
include $(BUILD_EXECUTABLE)
