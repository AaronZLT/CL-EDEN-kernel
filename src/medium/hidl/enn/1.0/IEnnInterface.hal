package vendor.samsung_slsi.hardware.enn@1.0;

import IEnnCallback;

interface IEnnInterface {
    /* model based interface */
    init(IEnnCallback cb_sp) generates (int32_t ret);
    deinit() generates (int32_t ret);
    open_model(LoadParameter load_param) generates (SessionBufInfo sbi_ret);
    commit_execution_data(uint64_t model_id, InferenceData exec_data) generates (uint64_t ret);
    execute_model(vec<uint64_t> exec_id_list) generates (int32_t ret);
    close_model(uint64_t model_id) generates (int32_t ret);

    custom_interface(uint32_t identifier, GeneralParameter parameter) generates (GeneralParameterReturn ret);
};